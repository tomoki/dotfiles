Emacs*useXIM: false
URxvt.depth: 32
! URxvt*letterSpace: -1
URxvt*scrollBar: false
URxvt*termName: rxvt

URxvt.foreground: #D7D0C7
URxvt.background: #151515
! Black
URxvt.color0: #303030
URxvt.color8: #404040
! Red
URxvt.color1: #E84F4F
URxvt.color9: #D23D3D
! Green
URxvt.color2: #B8D68C
URxvt.color10: #A0CF5D
! Yellow
URxvt.color3: #E1AA5D
URxvt.color11: #F39D21
! Blue
URxvt.color4: #7DC1CF
URxvt.color12: #4E9FB1
! Magenta
URxvt.color5: #9B64FB
URxvt.color13: #8542FF
! Cyan
URxvt.color6: #6D878D
URxvt.color14: #42717B
! White
URxvt.color7: #dddddd
URxvt.color15: #dddddd


URxvt*font: xft:Migu 1M:size=12:antialias=true
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard,matcher
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.url-launcher: /bin/firefox
URxvt.matcher.button: 1

URxvt*preeditType: OnTheSpot 
URxvt*perl-ext: xim-onthespot 
